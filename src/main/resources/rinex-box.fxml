<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="233.0" prefWidth="620.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WrapperController">
  <children>
      <StackPane prefHeight="150.0" prefWidth="200.0">
         <children>
            <AnchorPane fx:id="mainPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="254.0" prefWidth="620.0">
            <children>
                  <TextField fx:id="inputTxtField" layoutX="23.0" layoutY="33.0" prefHeight="25.0" prefWidth="161.0" />
                  <TextField fx:id="outputTxtField" layoutX="22.0" layoutY="89.0" prefHeight="25.0" prefWidth="161.0" />
                  <Button fx:id="outputBtn" layoutX="82.0" layoutY="191.0" mnemonicParsing="false" onAction="#onOutputBtnClick" text="Вывод" />
                  <Text fx:id="fileTypesText" layoutX="286.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Выбор типов данных на выход" textAlignment="CENTER" wrappingWidth="121.0" />
                  <Text fx:id="satSysText" layoutX="486.0" layoutY="31.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Выбор систем" />
                  <Text layoutX="42.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Путь до исходного файла" />
                  <Text layoutX="44.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Папка выходного файла" />
                  <Button fx:id="inputFileSelectBtn" layoutX="190.0" layoutY="33.0" mnemonicParsing="false" onAction="#pickInputFile" text="..." />
                  <Button fx:id="outputFileSelectBtn" layoutX="190.0" layoutY="89.0" mnemonicParsing="false" onAction="#pickOutputFile" text="..." />
                  <Accordion layoutX="252.0" layoutY="33.0" />
                  <VBox fx:id="fileTypeVBox" layoutX="297.0" layoutY="58.0" prefHeight="170.0" prefWidth="100.0" />
                  <VBox fx:id="satSysVBox" layoutX="475.0" layoutY="57.0" prefHeight="170.0" prefWidth="100.0" />
                  <TextField fx:id="outputName" layoutX="31.0" layoutY="147.0" prefHeight="25.0" prefWidth="161.0" />
                  <Text layoutX="46.0" layoutY="137.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Имя выходного файла" />
            </children>
          </AnchorPane>
            <VBox fx:id="loadPane" disable="true" prefHeight="200.0" prefWidth="100.0" />
         </children>
      </StackPane>
  </children>
</VBox>
